l:int
n:int

fn main() -> int {
entry:
  $ret 0
}

fn test(l:int) -> int {
let _t1:int, _t2:int, _t3:int, n:int
bb1:
  _t1 = $cmp gt l 0
  $branch _t1 bb2 bb3

bb2:
  _t2 = $arith sub l 1
  l = $copy _t2
  _t3 = $arith mul n 2
  n = $copy _t3
  $jump bb1

bb3:
  $ret n

entry:
  n = $copy 1
  $jump bb1
}
